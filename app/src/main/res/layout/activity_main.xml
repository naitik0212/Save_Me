<?xml version="1.0" encoding="utf-8"?>


    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#324646">

    <TextView
        android:id="@+id/headertext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:shadowColor="#000000"
        android:text="Need a Doctor.. Click Save Me!"
        android:textAllCaps="true"
        android:textColor="#ffffff"
        android:textColorHighlight="#ffffff"
        android:textSize="22dp"
        android:textStyle="italic" />


    <!--<Button-->
        <!--android:id="@+id/SaveMeUser"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentStart="true"-->
        <!--android:layout_alignParentTop="true"-->
        <!--android:background="@drawable/buttoncolors"-->
        <!--android:text="Save Me" />-->

    <ImageButton
        android:id="@+id/SaveMeUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="#324646"
        android:layout_below="@+id/headertext"
        android:contentDescription="Save Me"
        android:src="@drawable/ic_home_save_me" />


    <TextView
        android:id="@+id/DocList"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_below="@+id/SaveMeUser"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:text="..." />

    <Button
        android:id="@+id/CancelSaveMeUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/headertext"
        android:textColor="#ffffff"
        android:layout_below="@+id/DocList"
        android:background="@drawable/buttoncolors1"
        android:text="Cancel Request" />


    <Button
        android:id="@+id/CompleteSaveMeUser"
        android:layout_width="wrap_content"
        android:textColor="#ffffff"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/headertext"
        android:layout_below="@+id/DocList"
        android:layout_marginEnd="12dp"
        android:background="@drawable/buttoncolors2"
        android:text="Complete Request" />

    <ImageButton
        android:id="@+id/call911"
        android:layout_width="40dp"
        android:layout_height="45dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/CompleteSaveMeUser"
        android:layout_marginBottom="12dp"
        android:layout_marginStart="11dp"
        android:background="#324646"
        android:contentDescription="Call 911"
        android:src="@drawable/ic_call911" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/CancelSaveMeUser"
        android:layout_alignTop="@+id/call911"
        android:shadowColor="#000000"
        android:text="Call 911 - >"
        android:textAllCaps="true"
        android:textColor="#ffffff"
        android:textColorHighlight="#ffffff"
        android:textSize="22dp"
        android:textStyle="italic"
        android:visibility="visible" />



