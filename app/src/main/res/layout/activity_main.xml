    <Button
        android:id="@+id/LogoutUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:layout_alignEnd="@+id/CompleteSaveMeUser"
        android:layout_alignTop="@+id/call911"
        android:background="@drawable/buttoncolors"
        android:text="Logout" />


</RelativeLayout>
